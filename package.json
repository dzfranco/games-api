{
	"name": "ultra-task",
	"version": "1.0.0",
	"license": "MIT",
	"description": "Task for ultra",
	"main": "index.js",
	"scripts": {
		"compile-native:stg": "cross-env NODE_ENV=staging node build.js && tsc",
		"compile-native:prod": "cross-env NODE_ENV=production node build.js && tsc",
		"dev": "npm install && NODE_ENV=development node build.js && backpack dev --inspect=0.0.0.0:5858",
		"lint": "npm run tslint -- -o reports/lint_issues.json -t json --force \"server/**/*.ts\" ",
		"tslint": "tslint",
		"test": "NODE_ENV=test node build.js && npm run lint && jest --no-cache --maxWorkers=4 --forceExit --config jest.config.json --detectOpenHandles --coverage"
	},
	"devDependencies": {
		"@types/dotenv": "8.2.0",
		"@types/express": "^4.16.1",
		"@types/graphql": "^14.2.0",
		"@types/jest": "^24.0.5",
		"@types/lodash": "^4.14.133",
		"@types/request": "^2.48.1",
		"@types/supertest": "^2.0.7",
		"awesome-typescript-loader": "^5.2.1",
		"backpack-core": "^0.8.3",
		"concurrently": "^5.0.0",
		"jest": "24.8.0",
		"prettier": "1.19.1",
		"shelljs": "^0.8.3",
		"supertest": "^4.0.2",
		"ts-jest": "^24.0.2",
		"ts-node": "^8.3.0",
		"tslint": "^5.18.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-config-standard": "^8.0.1",
		"typedoc": "^0.15.0",
		"typedoc-webpack-plugin": "^1.1.4",
		"typescript": "^3.3.3",
		"wait-port": "^0.2.2",
		"yarn": "1.21.1"
	},
	"dependencies": {
		"@types/faker": "^4.1.0",
		"@types/restify-errors": "^4.3.3",
		"date-fns": "^2.8.1",
		"dotenv": "8.2.0",
		"express": "4.17.1",
		"express-class-validator": "^1.0.3",
		"express-error-handler": "^1.1.0",
		"express-partial-response": "1.0.0",
		"express-validator": "^6.3.0",
		"faker": "^4.1.0",
		"inversify": "5.0.1",
		"inversify-express-utils": "6.3.2",
		"mysql2": "^2.0.2",
		"path": "0.12.7",
		"reflect-metadata": "0.1.13",
		"response-time": "2.3.2",
		"restify-errors": "^8.0.2",
		"typeorm": "^0.2.21"
	},
	"peerDependencies": {
		"class-validator": "0.11.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"jsx",
			"json",
			"ts",
			"tsx"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "ts-jest"
		},
		"testMatch": [
			"/**/*.spec.(ts)"
		],
		"testEnvironment": "node",
		"collectCoverage": true,
		"coverageReporters": [
			"lcov"
		],
		"coverageDirectory": "coverage",
		"collectCoverageFrom": [
			"**/server/**/*.{ts}",
			"!**/node_modules/**",
			"!**/build/**"
		]
	}
}
